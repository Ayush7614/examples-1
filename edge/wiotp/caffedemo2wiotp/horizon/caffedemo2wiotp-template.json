{
  "org": "$HZN_ORG_ID",
  "label": "Caffedemo2wiotp for $ARCH2",
  "description": "Sample Horizon workload that repeatedly queries a Deep Learning service and sends results to WIoTP",
  "public": true,
  "workloadUrl": "https://internetofthings.ibmcloud.com/workloads/caffedemo2wiotp",
  "version": "$CAFFEDEMO2WIOTP_VERSION",
  "arch": "$ARCH2",
  "downloadUrl": "not used yet",
  "apiSpec": [
    {
      "specRef": "https://internetofthings.ibmcloud.com/microservices/caffedemo",
      "org": "$HZN_ORG_ID",
      "version": "[0.0.0,INFINITY)",
      "arch": "$ARCH2"
    },
    {
      "specRef": "https://internetofthings.ibmcloud.com/wiotp-edge/microservices/edge-core-iot-microservice",
      "org": "IBM",
      "version": "[1.0.11,INFINITY)",
      "arch": "$ARCH2"
    }
  ],
  "userInput": [],
  "workloads": [
    {
    "deployment": {
      "services": {
        "caffedemo2wiotp": {
          "image": "$DOCKER_HUB_ID/${ARCH2}_example_wl_caffedemo2wiotp:$CAFFEDEMO2WIOTP_VERSION",
          "binds": [
            "/var/wiotp-edge:/var/wiotp-edge"
          ],
          "environment": [
            "WIOTP_DOMAIN=$WIOTP_DOMAIN",
            "WIOTP_EDGE_MQTT_IP=edge-connector",
            "WIOTP_PEM_FILE=/var/wiotp-edge/persist/dc/ca/ca.pem"
          ]
        }
      }
    },
    "deployment_signature": "",
    "torrent": "{\"url\":\"\",\"signature\":\"\"}"
    }
  ]
}
