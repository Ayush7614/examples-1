{
  "label": "Mic2wiotp for x86, arm, and arm64",
  "description": "Horizon deployment pattern that runs the mic2wiotp workload",
  "public": true,
  "workloads": [
    {
      "workloadUrl": "https://internetofthings.ibmcloud.com/workloads/mic2wiotp",
      "workloadOrgid": "$WIOTP_ORG_ID",
      "workloadArch": "amd64",
      "workloadVersions": [
        {
          "version": "$MIC2WIOTP_VERSION",
          "deployment_overrides": "{\"services\":{\"mic2wiotp\":{\"environment\":[\"WIOTP_DOMAIN=${WIOTP_TEST_ENV2}internetofthings.ibmcloud.com\",\"WIOTP_EDGE_MQTT_IP=${WIOTP_EDGE_MQTT_IP}\",\"VERBOSE=1\"]}}}",
          "deployment_overrides_signature": "",
          "priority": {},
          "upgradePolicy": {}
        }
      ],
      "nodeHealth": {
        "missing_heartbeat_interval": 600,
        "check_agreement_status": 120
      }
    },
    {
      "workloadUrl": "https://internetofthings.ibmcloud.com/workloads/mic2wiotp",
