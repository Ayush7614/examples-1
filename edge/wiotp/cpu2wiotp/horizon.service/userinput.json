{
    "services": [
        {
            "org": "$HZN_ORG_ID",
            "url": "https://$MYDOMAIN/services/$CPU_NAME",
            "versionRange": "[0.0.0,INFINITY)",
            "variables": {}
        },
        {
            "org": "IBM",
            "url": "https://internetofthings.ibmcloud.com/wiotp-edge/services/core-iot",
            "versionRange": "[0.0.0,INFINITY)",
            "variables": {
                "WIOTP_CLIENT_ID": "g:$HZN_ORG_ID:$WIOTP_GW_TYPE:$WIOTP_GW_ID",
                "WIOTP_DEVICE_AUTH_TOKEN": "$WIOTP_GW_TOKEN",
                "WIOTP_DOMAIN": "$HZN_ORG_ID.messaging.$WIOTP_DOMAIN"
            }
        },
        {
            "org": "$HZN_ORG_ID",
            "url": "https://$MYDOMAIN/services/$CPU2WIOTP_NAME",
            "versionRange": "[0.0.0,INFINITY)",
            "variables": {
                "MOCK": false,
                "PUBLISH": true,
                "SAMPLE_INTERVAL": 2,
                "SAMPLE_SIZE": 5,
                "VERBOSE": "1"
            }
        }
    ]
}