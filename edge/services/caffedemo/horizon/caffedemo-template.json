    {
      "deployment": {
        "services": { 
          "caffedemo": { 
            "image": "$DOCKER_HUB_ID/${ARCH2}_example_ms_caffedemo:${CAFFEDEMO_VERSION}",
            "privileged": true,
            "binds": [
              "/dev:/dev:ro"
            ],
            "userinput": {
              "name": "CAFFEDEMO_CAMERA_DEVICENUM",
              "label": "preferred device number for local camera. (0=/dev/video0, 1=/dev/video1, etc)",
              "type": "string",
              "defaultValue": "0"
            }
          }
        }
      },
      "deployment_signature": "",
      "torrent": "{\"url\":\"\",\"signature\":\"\"}"
    }
  ]
