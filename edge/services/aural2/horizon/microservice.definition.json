{
  "org": "$HZN_ORG_ID",
  "label": "$AURAL2_NAME for $ARCH",
  "description": "Provides a stream of user intents",
  "public": true,
  "specRef": "https://$MYDOMAIN/microservices/$AURAL2_NAME",
  "version": "$AURAL2_VERSION",
  "arch": "$ARCH",
  "sharable": "single",
  "userInput": [],
  "workloads": [{
    "deployment": {
      "services": {
        "microphone": {
          "image": "is8ac/example_ms_arm64_microphone:1.2.6",
          "privileged": true
        },
        "aural2": {
          "image": "is8ac/aural2_arm64:0.3.4",
          "binds": ["/var/aural2:/persist"],
          "specific_ports": [{
            "HostPort": "48125/tcp",
            "HostIP": "0.0.0.0"
          }]
        }
      }
    },
    "deployment_signature": "",
    "torrent": ""
  }]
}
